extends layout

block stylesheet
    link(rel="stylesheet", href="/assets/css/representative.css")

block sidebar
    #wrapper
        #top-space
            nav#top-space-nav-bar
                img.logo-pic(src='/assets/img/logo.png', alt='dash')
        a.mobilemenu(href='#sidebar')
            i.icon-reorder
        #sidebar
            #main-nav
                #nav-container
                    #welcome-repr
                        h2 Welcome
                    #profile.clearfix
                        .portrate.hidden-xs
                        .title
                            h2=name
                    ul#navigation
                        li
                            a(href='/representatives/dashboard')
                                .icon.icon-dashboard
                                    .fa.fa-address-card-o
                                .text Dashboard
                        li
                            a(href='#')
                                .icon.icon-manual
                                    .fa.fa-book
                                .text .......
                        li
                            a(href='#Settings')
                                .icon.icon-setting
                                    .fa.fa-cog
                                .text Settings
                        li.external
                            a(href='/logout')
                                .icon.icon-logout
                                    .fa.fa-sign-out
                                .text Logout
            .social-icons
                ul
                    li
                        a(href='#')
                            i.icon-facebook
                    li
                         a(href='#')
                            i.icon-twitter
                    li
                         a(href='#')
                             i.icon-linkedin
